{
  "name": "d-bugger",
  "version": "0.0.1",
  "scripts": {
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "fix": "eslint --ignore-path .gitignore . --fix && prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "dev": "eslint --ignore-path .gitignore . && svelte-kit dev",
    "build": "svelte-kit build"
  },
  "devDependencies": {
    "@macfja/svelte-multi-adapter": "^1.0.1",
    "@sveltejs/adapter-node": "^1.0.0-next.55",
    "@sveltejs/adapter-static": "^1.0.0-next.21",
    "@sveltejs/kit": "^1.0.0-next.202",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.11",
    "svelte-intersection-observer": "^0.9.2",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@google-cloud/secret-manager": "^3.10.1",
    "@tensorflow/tfjs-node": "^3.12.0",
    "firebase": "^9.6.1",
    "firebase-admin": "^9.12.0",
    "highlight.js": "^11.3.1",
    "nsfwjs": "^2.4.1",
    "piexifjs": "^1.0.6",
    "sanitize-html": "^2.6.1",
    "showdown": "^1.9.1",
    "xhr2": "^0.2.1"
