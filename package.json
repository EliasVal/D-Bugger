{
  "name": "d-bugger",
  "version": "0.0.1",
  "scripts": {
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "fix": "eslint --ignore-path .gitignore . --fix && prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "dev": "eslint --ignore-path .gitignore . && svelte-kit dev",
    "build": "svelte-kit build"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.46",
    "@sveltejs/kit": "^1.0.0-next.165",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss-cli": "^9.0.1",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-intersection-observer": "^0.9.0",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.17",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@google-cloud/secret-manager": "^3.10.1",
    "@tensorflow/tfjs-node": "^3.10.0",
    "dotenv": "^10.0.0",
    "firebase": "^9.1.3",
    "firebase-admin": "^10.0.0",
    "highlight.js": "^11.3.1",
    "nsfwjs": "^2.2.0",
    "piexifjs": "^1.0.6",
    "sanitize-html": "^2.5.2",
    "showdown": "^1.9.0"
  }
}
